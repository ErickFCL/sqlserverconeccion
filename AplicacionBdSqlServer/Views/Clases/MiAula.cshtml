


<html>
<head>
    <title>Upn</title>
</head>
<body>
    <h1>Bienvenido</h1>
    <hr />
    <button class="btn btn-success">
        <a href="/clases/registrarA">Registrar Matricula</a>
    </button>
    <div class="container">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Alumno</th>
                    <th>Curso</th>
                    <th>Profesor</th>
                </tr>
            </thead>

            <tbody>
                @{ int i = 0;}
                @foreach (var item in ViewBag.Cursos)
                {
                    i++;
                    <tr>
                        <td>@i</td>
                        <td>@item.Alumnos.Nombre</td>
                        <td>@item.Nombre</td>
                        <td>@item.Profesores.Nombre</td>
                    </tr>
                }

            </tbody>
           
        </table>
    </div>
</body>
</html>